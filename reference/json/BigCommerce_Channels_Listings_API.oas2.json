        "platform": {
          "type": "string",
          "description": "The name of the platform for the channel. Must be used with a valid platform combination",
          "enum": [
            "wordpress",
            "square",
            "drupal",
            "acquia",
            "bloomreach",
            "deity",
            "amazon",
            "facebook",
            "ebay",
            "google_shopping",
